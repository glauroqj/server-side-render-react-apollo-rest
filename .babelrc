{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "env": {
    "test": {
      "plugins": [
        "@babel/plugin-transform-runtime"
      ]
    },
    "development": {
      "plugins": [
        "@babel/plugin-transform-runtime",
        "babel-plugin-styled-components"
      ]
    },
    "production": {
      "plugins": [
        "transform-react-remove-prop-types",
        "babel-plugin-styled-components"
      ]
    }
  },
  "plugins": [
    [
      "module-resolver", {
        "root": ["."],
        "alias": {
          "infra": "./src/infra",
          "routes": "./src/routes",
          "components": "./src/components",
          "pages": "./src/pages",
          "assets": "./src/assets"
        },
        "cwd": "babelrc"
    }]
  ]
 }